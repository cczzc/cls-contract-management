<%@page import="UserSystem.Contract"%>
<%@page import="Check.Judge"%>
<%@ page import="java.sql.*" language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Insert title here</title>
<style>
#header {
    color:white;
    text-align:center;
    padding:0px;
}
</style>
</head>
<body>
<% 
    Judge jd = Judge.getJudge();
    Contract c = Contract.getContract();
%>
<div id="header">
<h3 style="margin-left:-150px">已过期合同</h3>
</div>

<form style="margin-left:862px" action="ExpiredContract.jsp">
<input type="submit" value="返回" style="width:60px;height:24px;font-size:14px;border:0px">
</form>
<p style="color:white">
合同名称 ：<input type="text" value="<%=c.getTitle()%>" name="ExpiredContractName" style="width:300px;height:20px" readonly="readonly">
</p>
<p style="color:white">
客户 &nbsp;&nbsp;：<input type="text" value="<%=c.getClient()%>" name="ExpiredClinetName" style="width:300px;height:20px" readonly="readonly">
</p>
<p style="color:white">
起始时间 ：<input type="text" value="<%=c.getStarttime()%>" name="ExpiredStartTime" style="width:300px;height:20px" readonly="readonly">
</p>
<p style="color:white">
结束时间 ：<input type="text" value="<%=c.getEndtime()%>" name="ExpiredEndTime" style="width:300px;height:20px" readonly="readonly">
</p>
<p style="color:white">
合同内容 ：
</p>
<p style="margin-left:87px">
<textarea id="Textarea" name="ExpiredContractContent" style="width:829px;height:300px;resize:none;outline:none" readonly="readonly"></textarea>
</p>
<div>
<div style="float:left"><form action="Delete.jsp" method="post"><input type="submit" value="删除" style="width:60px;height:24px;font-size:14px;border:0px">&nbsp;&nbsp;</form></div>
<div style="float:left"><form action="Welcome.jsp"><input type="submit" value="取消" style="width:60px;height:24px;font-size:14px;border:0px">&nbsp;&nbsp;</form></div>
</div>
<form style="margin-left:862px" action="ExpiredContract.jsp">
<input type="submit" value="返回" style="width:60px;height:24px;font-size:14px;border:0px">
</form>
<input type="hidden" id="Area" value="<%=c.getContent()%>"/> 
<script type="text/javascript">  
    document.getElementById("Textarea").value=document.getElementById("Area").value;   
</script> 
</body>
</html>
